{
  "typingOptions": {
    "enableAutoDiscovery": true,
  },
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "target": "es2015",
    "lib": [
      "es2015",
      "dom"
    ],
    "sourceMap": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "outDir": "dist"
  },
  "include": [
    "build/*",
    "src/.bpmnlintrc",
  ],
  "exclude": [
    "node_modules",
  ],
}
