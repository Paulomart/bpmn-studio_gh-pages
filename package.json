    "aurelia-bootstrap": "0.1.20",
    "aurelia-bootstrapper": "2.3.3",
    "aurelia-cli": "^1.2.2",
    "aurelia-protractor-plugin": "1.0.6",
    "aurelia-route-recognizer": "1.3.2",
    "aurelia-router": "1.7.1",
    "aurelia-templating-resources": "1.12.0",
    "aurelia-testing": "1.0.0",
    "aurelia-tools": "2.0.0",
    "browser-sync": "^2.26.7",
